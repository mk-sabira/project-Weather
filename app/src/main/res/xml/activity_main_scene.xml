<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_editor_absoluteY="69dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="159dp"
            android:layout_width="wrap_content"
            android:id="@+id/button"
            motion:layout_constraintTop_toTopOf="@+id/button2"
            motion:layout_constraintBottom_toTopOf="@+id/button2" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="159dp"
            android:id="@+id/button2"
            android:layout_marginTop="192dp"
            motion:layout_constraintBottom_toTopOf="@+id/button3"
            motion:layout_constraintTop_toTopOf="@+id/button3" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="150dp"
            android:id="@+id/button3"
            motion:layout_constraintTop_toBottomOf="@+id/button2"
            android:layout_marginTop="228dp"
            motion:layout_constraintBottom_toBottomOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>